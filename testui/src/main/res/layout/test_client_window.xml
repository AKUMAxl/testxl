<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center">

    <Button
        android:id="@+id/test_client_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BACK"/>

    <Button
        android:id="@+id/test_client_navi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="导航"/>

    <Button
        android:id="@+id/test_client_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="电话"/>

    <Button
        android:id="@+id/test_client_video"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="视频"/>

    <Button
        android:id="@+id/test_client_ui_control"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UI_Control"/>

</LinearLayout>